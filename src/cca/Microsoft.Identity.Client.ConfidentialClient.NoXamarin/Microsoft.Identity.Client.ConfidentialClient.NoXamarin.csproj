<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup Label="Target Frameworks"> 
    <TargetFrameworkNetDesktop461>net461</TargetFrameworkNetDesktop461> 
    <!--<TargetFrameworkNetStandard>netstandard2.0</TargetFrameworkNetStandard>-->
    <TargetFrameworkNetCore>netcoreapp2.1</TargetFrameworkNetCore>
    <TargetFrameworkNetDesktop45>net45</TargetFrameworkNetDesktop45>

    <TargetFrameworks>$(TargetFrameworkNetStandard);$(TargetFrameworkNetCore);</TargetFrameworks>
    <TargetFrameworks Condition="$([MSBuild]::IsOsPlatform('Windows'))">$(TargetFrameworks);$(TargetFrameworkNetDesktop45);$(TargetFrameworkNetDesktop461);</TargetFrameworks>
    
    <NoWarn>$(NoWarn);CS8002;NU5131;</NoWarn>
  </PropertyGroup>

  <Import Project="../../flags.props" />
  <Import Project="cca.references.props" />

  <ItemGroup>
    <Compile Include="$(MSBuildThisFileDirectory)\..\..\pca\Microsoft.Identity.Client.PublicClient.NoXamarin\Shared\**\*.cs" LinkBase="PCA_Shared" />
    <ProjectReference Include="..\..\shared\Microsoft.Identity.Client.Shared.NoXamarin\Microsoft.Identity.Client.Shared.NoXamarin.csproj" />
  </ItemGroup>

  
  
</Project>
