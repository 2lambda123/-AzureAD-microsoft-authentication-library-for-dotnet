{
  "version": "2.7-preview", 
  "assemblyVersion": {
    "precision": "revision"
  },
  "publicReleaseRefSpec": [
    "^refs/heads/master$", // we release out of master
    "^refs/tags/v\\d+\\.\\d+" // we also release tags starting with vN.N
  ],
  "nugetPackageVersion": {
    "semVer": 2
  },
   "cloudBuild": {
    "buildNumber": {   // use version as build number
      "enabled": true
    }
  }
}
