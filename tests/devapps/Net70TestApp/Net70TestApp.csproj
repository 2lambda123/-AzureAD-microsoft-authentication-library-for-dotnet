<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFrameworkNet70>net7.0</TargetFrameworkNet70>
    <TargetFramework>net7.0</TargetFramework>
    <Configurations>Debug;Release;Debug + MobileApps</Configurations>
    <DefineConstants>Net70App</DefineConstants>
  </PropertyGroup>

  <PropertyGroup Condition="'$(TargetFramework)' == '$(TargetFrameworkNet70)'">
    <DefineConstants>$(DefineConstants);NET70</DefineConstants>
  </PropertyGroup>

  <ItemGroup>
    <ProjectReference Include="..\..\..\src\client\Microsoft.Identity.Client\Microsoft.Identity.Client.csproj" />
    <ProjectReference Include="..\..\Microsoft.Identity.Test.Integration.netcore\Microsoft.Identity.Test.Integration.NetCore.csproj" />
  </ItemGroup>

  <ItemGroup>
    <Compile Include="..\..\Microsoft.Identity.Test.Common\TestConstants.cs" Link="Infrastructure\TestConstants.cs" />
    <Compile Include="..\..\Microsoft.Identity.Test.Integration.netfx\Infrastructure\MsalAssert.cs" Link="Infrastructure\MsalAssert.cs" />
    <Compile Include="..\..\Microsoft.Identity.Test.Integration.netfx\Infrastructure\TargetFramework.cs" Link="Infrastructure\TargetFramework.cs" />
    <Compile Include="..\NetCoreTestApp\Experimental\DefaultOsBrowserWebUi.cs" Link="Experimental\DefaultOsBrowserWebUi.cs" />
    <Compile Include="..\NetCoreTestApp\Experimental\SingleMessageTcpListener.cs" Link="Experimental\SingleMessageTcpListener.cs" />
    <Compile Include="..\NetCoreTestApp\Program.cs" Link="Program.cs" />
  </ItemGroup>

  <ItemGroup>
    <Folder Include="Experimental\" />
    <Folder Include="Infrastructure\" />
  </ItemGroup>

</Project>

